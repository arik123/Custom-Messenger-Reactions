{
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/inject-script.js" ],
      "matches": [ "https://www.facebook.com/messages/*", "http://www.facebook.com/messages/*", "https://www.messenger.com/*", "http://www.messenger.com/*", "https://www.facebook.com/*", "http://www.facebook.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "Adds all avaliable reaction to Facebook Messenger",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "64": "icons/icon64.png"
   },
   "manifest_version": 2,
   "name": "Additional reactions for Messenger",
   "version": "0.1",
   "web_accessible_resources": [ "js/main.js" ]
}
